<div class="case-study-frame">
	<ul class="case-study-nav">
	</ul>
    <div 	class="cycle-slideshow" 
		    data-cycle-fx="scrollHorz"
		    data-cycle-timeout="0"
		    data-cycle-prev="#prev"
		    data-cycle-next="#next"
		    data-cycle-slides="> section"
		    data-cycle-pager=".case-study-nav"
    >
    	{% for case in climate_leaders.get_field('case_study') %}
    		{% include 'views/sections/case_study_modal_slide.twig' %}
    	{% endfor %}
	</div>
	<div class="case-study-footer">
		<a class="close">Back To Main Page</a>
		<div class="download-icon">
			<div class="circle">
				{% include "views/svg/dl_icon.twig" %}
			</div>
		</div>
	</div>
</div>